---
title: Mentorship Platform - Swimlane Diagram
---
flowchart TB
    %% Student Swimlane
    subgraph StudentLane["ğŸ‘¨â€ğŸ“ STUDENT SWIMLANE"]
        direction TB
        S_START([Student Begins])
        S1[Student Login]
        S2[View Recommended<br/>Mentors]
        S3[Select Mentor &<br/>Book Session]
        S4[Make Payment<br/>via Stripe]
        S5{Payment<br/>Successful?}
        S6[Receive Calendly Link]
        S7[Session Booked]
        S_END([End])
        
        S_START --> S1
        S1 --> S2
        S2 --> S3
        S3 --> S4
        S4 --> S5
        S5 -->|Yes| S6
        S5 -->|No| S3
        S6 --> S7
        S7 --> S_END
    end
    
    %% Mentor Swimlane
    subgraph MentorLane["ğŸ‘¨â€ğŸ« MENTOR SWIMLANE"]
        direction TB
        M_START([Mentor Begins])
        M1[Mentor Registration]
        M2["System Creates Profile<br/>â”â”â”â”â”â”â”â”â”â”â”<br/>isLive: false<br/>haveAdminApproval: false"]
        M3[Access Admin's<br/>Calendly Link]
        M4[Schedule Interview<br/>with Admin]
        M5["Added to<br/>Request Queue"]
        M6[Mentor Login]
        M7{Has Admin<br/>Approval?}
        M8[Provide Personal Info &<br/>Calendly Profile Link]
        M9[Purchase<br/>Subscription]
        M10["System Updates Status<br/>â”â”â”â”â”â”â”â”â”â”â”<br/>isLive: true"]
        M11["Profile Active &<br/>Visible to Students"]
        M_END([End])
        
        M_START --> M1
        M1 --> M2
        M2 --> M3
        M3 --> M4
        M4 --> M5
        M5 --> M6
        M6 --> M7
        M7 -->|No| M6
        M7 -->|Yes| M8
        M8 --> M9
        M9 --> M10
        M10 --> M11
        M11 --> M_END
    end
    
    %% Admin Swimlane
    subgraph AdminLane["ğŸ”‘ ADMIN SWIMLANE"]
        direction TB
        A_START([Admin Begins])
        A1[Admin Login]
        A2[Setup & Provide<br/>Calendly Profile Link]
        A3[View Mentor<br/>Request Queue]
        A4[Review Mentor<br/>Application]
        A5{Approve<br/>Mentor?}
        A6["Approve Mentor<br/>â”â”â”â”â”â”â”â”â”â”â”<br/>haveAdminApproval: true"]
        A7[Reject Mentor<br/>Application]
        A_END([End])
        
        A_START --> A1
        A1 --> A2
        A2 --> A3
        A3 --> A4
        A4 --> A5
        A5 -->|Yes| A6
        A5 -->|No| A7
        A6 --> A_END
        A7 --> A_END
    end
    
    %% Cross-Lane Interactions (Message Flows)
    M11 -.->|"provides data to"| S2
    M3 -.->|"uses link from"| A2
    M5 -.->|"triggers notification"| A3
    A6 -.->|"enables"| M7
    S7 -.->|"creates relation"| DB[(Mentor-Student<br/>Relation)]
    M11 -.->|"data stored in"| DB
    
    %% Styling
    classDef studentStyle fill:#e3f2fd,stroke:#1976d2,stroke-width:3px,color:#000
    classDef mentorStyle fill:#fff3e0,stroke:#f57c00,stroke-width:3px,color:#000
    classDef adminStyle fill:#fce4ec,stroke:#c2185b,stroke-width:3px,color:#000
    classDef systemStyle fill:#f5f5f5,stroke:#616161,stroke-width:2px,color:#000
    classDef decisionStyle fill:#fff9c4,stroke:#f57f17,stroke-width:2px,color:#000
    classDef dataStyle fill:#e8f5e9,stroke:#388e3c,stroke-width:3px,color:#000
    classDef startEndStyle fill:#d1c4e9,stroke:#512da8,stroke-width:3px,color:#000
    
    class S1,S2,S3,S4,S6,S7 studentStyle
    class M1,M3,M4,M6,M8,M9,M11 mentorStyle
    class A1,A2,A3,A4,A7 adminStyle
    class M2,M5,M10,A6 systemStyle
    class S5,M7,A5 decisionStyle
    class DB dataStyle
    class S_START,S_END,M_START,M_END,A_START,A_END startEndStyle